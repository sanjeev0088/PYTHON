import math

def are_mutually_coprime(a, b, c):
  
  return math.gcd(math.gcd(a, b), c) == 1


num1 = int(input("Enter the first number: "))
num2 = int(input("Enter the second number: "))
num3 = int(input("Enter the third number: "))


if are_mutually_coprime(num1, num2, num3):
  print(f"The numbers {num1}, {num2}, and {num3} are mutually coprime.")
else:
  print(f"The numbers {num1}, {num2}, and {num3} are not mutually coprime.")
